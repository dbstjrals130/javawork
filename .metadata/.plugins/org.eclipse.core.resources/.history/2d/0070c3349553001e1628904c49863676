package Singleton.student;

public class StudentMain {

	public static void main(String[] args) {
		School school = School.getInstance();
		
		Student gildong = school.createStudent();
		Student sadong = school.createStudent();
		Student youngsu = school.createStudent();
		
		System.out.println("학번 : " + gildong.getStundentNum());
		System.out.println("학번 : " + sadong.getStundentNum());
		System.out.println("학번 : " + youngsu.getStundentNum());

	}

}
