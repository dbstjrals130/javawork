package Objectarray.dogs;

public class DogArray {

	public static void main(String[] args) {
		// Dog 인스턴스 4개를 생성해서 배열에 저장하고
		// 정보를 출력하세요.(get()함수 사용)
		
		Dog[] dogs = new Dog[4];
		
		for(int i = 0; i <dogs.length; i++) {
			System.out.println([i]);
		}
		
		dogs[0] = new Dog("시바견");
		dogs[1] = new Dog("포메라니안");
		dogs[2] = new Dog("리트리버");
		dogs[3] = new Dog("진돗개");
		
		dog.name("dogs");
		

	}

}
