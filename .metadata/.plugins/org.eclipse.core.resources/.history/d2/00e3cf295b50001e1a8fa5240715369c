package classes.constructor;

public class ps {
	String name;
	float height;
	float weight;
	
	public ps() {}
	
	public ps(String n, float h, float w) {
		name = n;
		height = h;
		weight = w;
	}
	
	public void ShowPersonInfo() {
		System.out.printf("이름 : %s, " + "키 : %.1f, "
						+ "몸무게 : %.1f\n", name, height, weight);
	}

}
