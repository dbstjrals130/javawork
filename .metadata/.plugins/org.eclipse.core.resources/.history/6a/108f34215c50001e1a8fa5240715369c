package classes.constructor;

public class Ns {

	int x;
	
	public Ns() {
		x = 4;
	}
	
	public Ns(int y) {
		x = y;
	}
	
	public static void main(String[] args) {
		Ns myNum = new Ns();
		System.out.println(myNum.x);
		
		System.out.println("==============");
		
		Ns myNum2 = new Ns(10);
		System.out.println(myNum.2);
	}
	
	
	
	
	
}
